<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.StackPane?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="1200.0"
           xmlns="http://javafx.com/javafx/21-internal" fx:controller="org.mithras.mithras.MainController">
    <children>
        <Button mnemonicParsing="false" onAction="#switchToMain" text="Back" StackPane.alignment="BOTTOM_CENTER"
                fx:id="backbtn">
            <StackPane.margin>
                <Insets bottom="25.0"/>
            </StackPane.margin>
        </Button>
    </children>
</StackPane>
