<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
           prefHeight="1000.0" prefWidth="1200.0" style="-fx-background-color: black;"
           xmlns="http://javafx.com/javafx/21.0.1" fx:controller="org.mithras.mithras.NeuralNetworkLayerSelection">
    <children>
        <ScrollPane id="nnscrollpane" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: black;"/>
        <Button mnemonicParsing="false" onAction="#switchToMain" text="Back" StackPane.alignment="BOTTOM_CENTER">
            <StackPane.margin>
                <Insets bottom="25.0" left="120.0"/>
            </StackPane.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#switchToLayerView" text="Switch to Code View"
                StackPane.alignment="BOTTOM_CENTER">
            <StackPane.margin>
                <Insets bottom="25.0" right="120.0"/>
            </StackPane.margin>
        </Button>
    </children>
</StackPane>
