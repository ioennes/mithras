<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" xmlns="http://javafx.com/javafx/21.0.1">
    <children>
        <GridPane fx:id="inputgrid" style="-fx-background-color: black;">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <Button mnemonicParsing="false" onAction="#ok" text="Confirm" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="15.0" right="90.0"/>
            </StackPane.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" StackPane.alignment="BOTTOM_RIGHT">
            <StackPane.margin>
                <Insets bottom="15.0" right="15.0"/>
            </StackPane.margin>
        </Button>
    </children>
</StackPane>
